        rpy="0 0 0" />
      <mass value="${1.525883*density}"/>
      <inertia
      ixx="${0.00527835*density}" ixy="${0.00124150*density}" ixz="${-0.00000018*density}"
      iyy="${0.00317861*density}" iyz="${-0.00000067*density}"
      izz="${0.00652527*density}"/>
