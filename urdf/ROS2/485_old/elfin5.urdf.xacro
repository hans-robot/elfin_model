      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="file://$(find elfin_description)/meshes/elfin5/elfin_link5.STL" />
      </geometry>
      <material
        name="">
        <color
          rgba="0.686274509803922 0.686274509803922 0.686274509803922 1" />
      </material>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="file://$(find elfin_description)/meshes/elfin5/elfin_link5.STL" />
      </geometry>
    </collision>
  </link>
  <joint
    name="elfin_joint5"
    type="revolute">
    <origin
      xyz="-0.0605 0 0.301"
      rpy="-1.5708 0 1.5708" />
    <parent
      link="elfin_link4" />
    <child
      link="elfin_link5" />
   <axis
      xyz="0 0 -1" />
