    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="file://$(find elfin_description)/meshes/elfin5/elfin_link3.STL" />
      </geometry>
      <material
        name="">
        <color
          rgba="0.686274509803922 0.686274509803922 0.686274509803922 1" />
      </material>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="file://$(find elfin_description)/meshes/elfin5/elfin_link3.STL" />
      </geometry>
    </collision>
  </link>
  <joint
    name="elfin_joint3"
    type="revolute">
    <origin
      xyz="-0.37262 0.074541 -0.0060028"
